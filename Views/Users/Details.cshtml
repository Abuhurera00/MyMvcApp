@model MyMvcApp.Models.User
@{
    ViewData["Title"] = "User Details";
}

<h1>Details</h1>
<dl class="row">
    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Name)</dt>
    <dd class="col-sm-10">@Model.Name</dd>

    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Age)</dt>
    <dd class="col-sm-10">@Model.Age</dd>

    <dt class="col-sm-2">@Html.DisplayNameFor(m => m.Address)</dt>
    <dd class="col-sm-10">@Model.Address</dd>
</dl>
<a asp-action="Index" class="btn btn-secondary">Back to List</a>
